
set terminal postscript eps enhanced
set output "on_demand.eps"
set boxwidth 0.75 absolute
set style fill pattern 1.00 border -1
set style data histogram
set style histogram cluster gap 1
set xtics 10 nomirror
set ytics 10 nomirror
set mxtics 2
set mytics 2
set title "Effect of On Demand Allocation on 4KB stride Access"
#set logscale y
set ytics 0.1,0.2,2
set yrange [0.1:2.0]
#set logscale y
#set xrange [-1:4]
set ylabel "Normalized Time(us)"
set xlabel "Memory Allocated (MB)"

plot 'histdata-4k' using 2 t "4KB Page Sequentail", '' using 3:xtic(1) t "4KB Page Random", \
	'' using 4 t "2MB Page Sequentail", '' using 5 t "2MB Page Random"
#, '' using 5:xtic(1) t "Network"

#pause -1 "Hit any key to continue"

